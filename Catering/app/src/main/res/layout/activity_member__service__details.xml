<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background2"
    tools:context=".Member_Service_Details">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="G K Catering"
        android:gravity="center"
        android:textStyle="bold"
        android:textColor="@android:color/white"
        android:background="@drawable/text_back"
        android:textSize="30dp"
        android:id="@+id/logo"
        ></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="25dp"
        android:textColor="@android:color/holo_red_dark"
        android:text="Name : "
        android:layout_below="@+id/logo"
        android:layout_marginTop="15dp"
        android:textStyle="bold"
        android:id="@+id/name"
        ></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="25dp"
        android:textColor="@android:color/holo_red_dark"
        android:text="Contact Number : "
        android:layout_below="@+id/name"
        android:textStyle="bold"
        android:id="@+id/number"
        ></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="25dp"
        android:textColor="@android:color/holo_red_dark"
        android:text="Residential Place : "
        android:layout_below="@+id/number"
        android:textStyle="bold"
        android:id="@+id/place"
        ></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="25dp"
        android:textColor="@android:color/background_dark"
        android:text="Attended Events : "
        android:layout_below="@+id/place"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        android:id="@+id/head"
        ></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="30dp"
        android:textColor="@android:color/holo_red_dark"
        android:layout_below="@+id/head"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/warn1"
        ></TextView>
    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/services"
        android:layout_below="@+id/head"
        android:layout_marginTop="60dp"
        ></ListView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="25dp"
        android:textColor="@android:color/background_dark"
        android:text="Events to be Attended in Future: "
        android:layout_below="@+id/services"
        android:layout_marginTop="50dp"
        android:textStyle="bold"
        android:id="@+id/head2"
        ></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="30dp"
        android:textColor="@android:color/holo_red_dark"
        android:layout_below="@+id/head2"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/warn2"
        ></TextView>
    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/services2"
        android:layout_below="@+id/head2"
        android:layout_marginTop="70dp"
        ></ListView>

</RelativeLayout>